{
  "name": "semantic-bus",
  "version": "0.9.1",
  "description": "ETL style data middlware transformation embended in an ESB for all kind of data",
  "private": true,
  "main": "",
  "scripts": {
    "test": "jest --detectOpenHandles --verbose",
    "lint": "npx eslint *.js utils/**/*.js services/**/*.js workspaceComponentExecutor/**/*.js communication/**/*.js",
    "fix": "npx eslint --fix *.js utils/**/*.js services/**/*.js workspaceComponentExecutor/**/*.js communication/**/*.js",
    "start": "node app.js",
    "mongoPull": "sudo docker pull mongo:3.6",
    "mongoClean": "sudo docker volume rm data",
    "mongoRun": "sudo docker run -d -v data:/data/db -p 27017:27017 --name mongo mongo:3.6",
    "mongoStop": "sudo docker stop mongo && sudo docker rm mongo",
    "amqpPull": "sudo docker pull semanticbus/rabbitmq-stomp",
    "amqpStop": "sudo docker stop rabbitmq && sudo docker rm rabbitmq",
    "amqpRun": "sudo docker run -d -p 15672:15672 -p 15674:15674 -p 5672:5672 --name rabbitmq semanticbus/rabbitmq-stomp",
    "amqpRun2": "sudo docker run -d -p 15674:15674 -P --name rabbitmq semanticbus/rabbitmq-stomp",
    "dockerBuild": "sudo docker build -t semanticbus-app .",
    "dockerPush": "sudo docker tag semanticbus-app semanticbus/semanticbus && sudo docker push semanticbus/semanticbus",
    "dockerStop": "sudo docker stop semanticbus-app && sudo docker rm semanticbus-app",
    "dockerRun": "sudo docker run -i -P -p 8080:80 --link rabbitmq --name semanticbus-app semanticbus-app",
    "dockerRun2": "sudo docker run -i -P -p 8080:80 --link rabbitmq --name semanticbus-app -e CONFIG_URL='http://data-players.com/config/devButProd.json' semanticbus-app",
    "dockerRunProd": "sudo docker run -i -P -p 8080:80 --link rabbitmq --name semanticbus-app -e CONFIG_URL='http://data-players.com/config/prod.json' semanticbus-app"
  },
  "author": "Simon Louvet <simon.louvet.zen@gmail.com> && Boceno Alexandre <bocenoalex@gmail.com>",
  "license": "GNUV3",
